&CONTROL
   calculation = 'scf'
   prefix = 'grafeno_1',
   pseudo_dir = './examples/pseudos',
   outdir = './examples/output/example_1'
/
&system
    ibrav = 4,
    celldm(1) = 4.656, ! Ajustar según corresponda
