&CONTROL
   calculation = 'scf'
   prefix = 'grafeno_1',
   pseudo_dir = './examples/pseudos',
   outdir = './examples/output/example_3'
/
&system
    ibrav = 4,
    celldm(1) = 2.46, ! Longitud de celda en Ångström
    celldm(3) = 10.0, ! Separación entre capas en unidades de celldm(1)
    nat = 2,
    ntyp = 1,
  ecutwfc = 55.0,
    ecutrho = 400.0
/
&electrons
    conv_thr = 1.0d-8,
    mixing_beta = 0.7
/
ATOMIC_SPECIES
    C  12.0107  C.pz-vbc.UPF
ATOMIC_POSITIONS {alat}
    C 0.000000 0.000000 0.000000
    C 0.666667 0.333333 0.000000
K_POINTS automatic
  1 1 1   0 0 1   